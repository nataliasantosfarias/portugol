programa {
    funcao inicio() {
        inteiro numero1
        inteiro numero2

        escreva("Digite o primeiro número: \n")
        leia(numero1)

        escreva("Digite o segundo número: \n")
        leia(numero2)

        se (numero1 == numero2) {
            escreva("Os números são iguais.\n")
        } senao {
            escreva("Os números são diferentes cacete.\n")
            
        }
    }
}
