programa {
  funcao inicio() {
    // conta de 6 + 8 +10 + 12 .. até 100 de 2 em 2
    inteiro soma = 0
    inteiro i = 6

    // Loop para somar os números de 6 até 100 de 2 em 2
    enquanto (i <= 100) {
        soma = soma + i
        i = i + 2
    }

    // Exibição do resultado da soma
    escreva("A soma dos números de 6 a 100 de 2 em 2 é: \n", soma)
  }
}



